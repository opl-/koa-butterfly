{
  "name": "koa-butterfly",
  "version": "0.1.1",
  "description": "A feature packed Koa router.",
  "homepage": "https://github.com/opl-/koa-butterfly#readme",
  "bugs": {
    "url" : "https://github.com/opl-/koa-butterfly/issues"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/opl-/koa-butterfly.git"
  },
  "keywords": [
    "koa",
    "router",
    "middleware",
    "typescript",
    "koa-router"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "lib/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "nyc --report=lcov ava",
    "prepublishOnly": "npm test && npm run build"
  },
  "author": "opl",
  "license": "MIT",
  "engines": {
    "node": ">= 10.0.0"
  },
  "dependencies": {
    "@types/koa": "^2.11.4",
    "@types/koa-compose": "^3.2.5",
    "koa-compose": "^4.1.0"
  },
  "devDependencies": {
    "ava": "^3.12.1",
    "koa": "^2.13.0",
    "nyc": "^15.1.0",
    "typescript": "~4.0.2",
    "ts-node": "^9.0.0"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
